<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="ttk.Frame" id="Main_Frame">
    <property name="height">200</property>
    <property name="takefocus">true</property>
    <property name="width">200</property>
    <layout>
      <property name="column">0</property>
      <property name="propagate">True</property>
      <property name="row">0</property>
      <property name="sticky">w</property>
    </layout>
    <child>
      <object class="ttk.Button" id="GameLauncher">
        <property name="command">launchnextgame</property>
        <property name="default">normal</property>
        <property name="takefocus">true</property>
        <property name="text" translatable="yes">Install/launch game</property>
        <layout>
          <property name="column">0</property>
          <property name="ipadx">5</property>
          <property name="ipady">5</property>
          <property name="padx">5</property>
          <property name="pady">5</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="TimerStarter">
        <property name="command">timerstart</property>
        <property name="default">normal</property>
        <property name="text" translatable="yes">Start Timer</property>
        <layout>
          <property name="column">1</property>
          <property name="ipadx">5</property>
          <property name="ipady">5</property>
          <property name="padx">5</property>
          <property name="pady">5</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="stop_music">
        <property name="command">stop_music</property>
        <property name="state">normal</property>
        <property name="text" translatable="yes">Stop Music</property>
        <layout>
          <property name="column">2</property>
          <property name="ipadx">5</property>
          <property name="ipady">5</property>
          <property name="padx">5</property>
          <property name="pady">5</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">e</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
